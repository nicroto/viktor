@import "synth/*"
@import "patch-library/*"

body #dawContainer
	position: relative
	width: $panel-width
	height: $panel-height
	margin-top: 50px
	margin-left: auto
	margin-right: auto
	padding-bottom: 195px
	border-radius: 14px
	background-position: 50% 50%
	background-repeat: none
	background-image: url("../images/synth-base-panel.jpg")
	background-size: contain
	color: #eee
	#bottomRow
		position: absolute
		bottom: 0
		width: 100%